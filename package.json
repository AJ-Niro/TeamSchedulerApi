{
  "name": "team-scheduler-api",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "lint:test": "eslint .",
    "prettier:test": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "prepare": "husky",
    "typeorm:cli": "tsx --require tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm:cli migration:create src/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm:cli migration:run -- -d src/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm:cli migration:revert -- -d src/config/typeorm.config.ts",
    "migration:show": "npm run typeorm:cli migration:show -- -d ./src/config/typeorm.config.ts",
    "test:unit": "jest",
    "test:coverage": "jest --coverage --coverageThreshold='{\"global\": {\"statements\": 80, \"branches\": 80, \"functions\": 80, \"lines\": 80}}'"
  },
  "author": "AndresJoaquinNino",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@jest/globals": "29.7.0",
    "@types/eslint__js": "8.42.3",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.9.0",
    "dotenv": "16.4.5",
    "eslint": "9.14.0",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "typescript-eslint": "8.14.0"
  },
  "dependencies": {
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "express": "4.21.1",
    "inversify": "6.1.5",
    "pg": "8.13.1",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.20"
  }
}
